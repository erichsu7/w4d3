<h1>Request for <%= @crr.cat.name %></h1>

<table>
  <tr>
    <th>Attribute</th>
    <th>Value</th>
  </tr>
  <% @crr.attributes.each do |(attr, value)| %>
  <tr>
    <td><%= attr %></td>
    <td><%= value %></td>
  </tr>
  <% end %>
</table>

<a href= "<%= new_cat_rental_request_url %>">Request a cat now!</a>
